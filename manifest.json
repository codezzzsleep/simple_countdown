{
    "name":"simple countdown",
    "version":"1.0",
    "manifest_version": 3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "activeTap",
        "scripting",
        "runtime",
        "notifications"
    ],
    "action":{
        "default_icon":"images/icon.png",
        "default_popup":"popup.html"
    }
}
